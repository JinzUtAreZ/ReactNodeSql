<!DOCTYPE html>
<html lang="en">

<head>
    <title>CONNECTING SQL SERVER FROM NODE JS</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>

<body>

    <div class="container">

        <table class="table">
            <thead>
                <tr>
                    <th>Status</th>
                    <th>Start Time</th>
                    <th>End Time</th>
                    <th>Location</th>
                    <th>RowID</th>
                    <th>Dock</th>
                    <!-- <th>COMM</th>
                    <th>DEPTNO</th> -->
                </tr>
            </thead>
            <tbody>
                <% for(var i=0;i<model.length;i++){ %>
                    <tr>
                        <td>
                            <%=model[i].StatusDesc %>
                        </td>
                        <td>
                            <%=model[i].StartTime %>
                        </td>
                        <td>
                            <%=model[i].EndTime %>
                        </td>
                        <td>
                            <%=model[i].LocDesc %>
                        </td>
                        <td>
                            <%=model[i].RowID %>
                        </td>
                        <td>
                            <%=model[i].DockName %>
                        </td>
                        <!-- <td>
                            <%=model[i].COMM %>
                        </td>
                        <td>
                            <%=model[i].DEPTNO %>
                        </td> -->
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>

</body>

</html>